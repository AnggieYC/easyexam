AA,CAAA,SAAA,GAAA,8BAAA;AACA,QAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AACA,SAFA,EAEA,KAFA,CAEA,UAAA,CAAA;AAAA,iBAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAAA;AAAA,SAFA;AAGA,OANA;AAOA;AAvBA;AAzEA,CAAA","sourcesContent":["<template>\n<div class=\"container\">\n  <b-container fluid>\n    <!-- User Interface controls -->\n    <b-row>\n      <b-col sm=\"5\" md=\"6\" class=\"my-1\">\n        <b-form-group\n          label=\"Per page\"\n          label-cols-sm=\"6\"\n          label-cols-md=\"4\"\n          label-cols-lg=\"3\"\n          label-align-sm=\"right\"\n          label-size=\"sm\"\n          label-for=\"perPageSelect\"\n          class=\"mb-0\"\n        >\n          <b-form-select\n            v-model=\"perPage\"\n            id=\"perPageSelect\"\n            size=\"sm\"\n            :options=\"pageOptions\"\n          ></b-form-select>\n        </b-form-group>\n      </b-col>\n\n      <b-col sm=\"7\" md=\"6\" class=\"my-1\">\n        <b-pagination\n          v-model=\"currentPage\"\n          :total-rows=\"totalRows\"\n          :per-page=\"perPage\"\n          align=\"fill\"\n          size=\"sm\"\n          class=\"my-0\"\n        ></b-pagination>\n      </b-col>\n    </b-row>\n\n    <!-- Main table element -->\n    <b-table\n      show-empty\n      small\n      stacked=\"md\"\n      :items=\"items\"\n      :fields=\"fields\"\n      :current-page=\"currentPage\"\n      :per-page=\"perPage\"\n      :filter=\"filter\"\n      :filterIncludedFields=\"filterOn\"\n      :sort-by.sync=\"sortBy\"\n      :sort-desc.sync=\"sortDesc\"\n      :sort-direction=\"sortDirection\"\n      @filtered=\"onFiltered\"\n    >\n      <template v-slot:cell(name)=\"row\">\n        {{ row.value.first }} {{ row.value.last }}\n      </template>\n\n      <template v-slot:cell(actions)=\"row\">\n        <b-button\n          size=\"sm\"\n          @click=\"info(row.item, row.index, $event.target)\"\n          class=\"mr-1\"\n        >\n          Info\n        </b-button>\n        <b-button size=\"sm\"\n          class=\"mr-1\"\n          @click=\"deletep(row.item, row.index, $event.target)\"\n          variant=\"danger\"\n        >\n          Delete\n        </b-button>\n      </template>\n\n      <template v-slot:row-details=\"row\">\n        <b-card>\n          <ul>\n            <li v-for=\"(value, key) in row.item\" :key=\"key\">\n              {{ key }}: {{ value }}\n            </li>\n          </ul>\n        </b-card>\n      </template>\n    </b-table>\n\n    <!-- Info modal -->\n    <b-modal\n      :id=\"infoModal.id\"\n      :title=\"infoModal.title\"\n      ok-only\n      @hide=\"resetInfoModal\"\n    >\n      <pre>{{ infoModal.content }}</pre>\n    </b-modal>\n  </b-container>\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      items: [\n        {\n          isActive: true,\n          age: 40,\n          name: { first: \"Dickerson\", last: \"Macdonald\" }\n        },\n      ],\n      fields: [\n        {\n          key: \"id\",\n          label: \"ID\",\n          sortable: true,\n          sortDirection: \"desc\"\n        },\n        {\n          key: \"title\",\n          label: \"Title\",\n          sortable: true,\n          class: \"text-center\"\n        },\n        {\n          key: \"score\",\n          label: \"Score\",\n          sortable: true,\n          class: \"text-center\"\n        },\n        {\n          key: \"qualifiers\",\n          label: \"Qualifiers\",\n          sortable: true,\n          class: \"text-center\"\n        },\n        { key: \"actions\", label: \"Actions\" }\n      ],\n      totalRows: 1,\n      currentPage: 1,\n      perPage: 5,\n      pageOptions: [5, 10, 15],\n      sortBy: \"\",\n      sortDesc: false,\n      sortDirection: \"asc\",\n      filter: null,\n      filterOn: [],\n      infoModal: {\n        id: \"info-modal\",\n        title: \"\",\n        content: \"\"\n      }\n    };\n  },\n  computed: {\n    sortOptions() {\n      // Create an options list from our fields\n      return this.fields\n        .filter(f => f.sortable)\n        .map(f => {\n          return { text: f.label, value: f.key };\n        });\n    }\n  },\n  mounted() {\n    // Set the initial number of items\n    this.totalRows = this.items.length;\n\n    var url = 'http://'+ this.$store.state.clientURL + '/problem/v1/reportedProblems'\n    axios.get(url).then(response => {\n        console.log(response)\n        this.items = response.data\n    }).catch(e => console.log(e))\n  },\n  methods: {\n    info(item, index, button) {\n      this.infoModal.title = `Row index: ${index}`;\n      this.infoModal.content = JSON.stringify(item, null, 2);\n      this.$root.$emit(\"bv::show::modal\", this.infoModal.id, button);\n    },\n    resetInfoModal() {\n      this.infoModal.title = \"\";\n      this.infoModal.content = \"\";\n    },\n    onFiltered(filteredItems) {\n      this.totalRows = filteredItems.length;\n      this.currentPage = 1;\n    },\n    deletep(item, index, button) {\n      console.log(item, index, button)\n      axios.delete('http://'+ this.$store.state.clientURL + '/problem/v1/deleteProblem?idProblem=' + item.id).then(response => {\n        console.log(response)\n        var url = 'http://'+ this.$store.state.clientURL + '/problem/v1/reportedProblems'\n        axios.get(url).then(response => {\n        this.items = response.data\n    }).catch(e => console.log(e))\n      })\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}